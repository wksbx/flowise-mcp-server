{
  "name": "flowise-mcp",
  "version": "1.0.0",
  "description": "MCP server for Flowise AI workflows",
  "main": "dist/index.js",
  "bin": {
    "flowise-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "docker:build": "docker build -t flowise-mcp .",
    "docker:run": "docker run -i --rm --add-host=host.docker.internal:host-gateway --env-file .env flowise-mcp"
  },
  "keywords": ["mcp", "flowise", "ai", "chatflow", "model-context-protocol", "llm"],
  "author": "YOUR_NAME",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOUR_USERNAME/flowise-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/flowise-mcp/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/flowise-mcp#readme",
  "packageManager": "pnpm@10.16.1",
  "type": "module",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "flowise-sdk": "^1.0.9",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3"
  }
}
